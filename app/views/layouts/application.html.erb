<!DOCTYPE html>
<html>
<head>
  <title>Wake 'N Shake</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
	<!-- Navigation Bar -->
	<nav class="navbar navbar-inverse">
		<div class="container">
			<div id="navbar-header">
				<ul class="nav navbar-nav">
					<li>
						<%= link_to "Wake 'N Shake", root_path, class: "navbar-brand"%>
					</li>
	        <li>
	        	<%= link_to "About Us", controller: :about, action: :index %>
	        </li>
					<li>
						<%= link_to "Donate", controller: :donations, action: :index %>
					</li>
					<li>
						<%= link_to "Dancers", dancers_path%>
					</li>
				</ul>
      	<ul class="nav navbar-nav navbar-right">
      		<li>
      			<%= link_to "Log In", controller: :dancers, action: :login %>
      		</li>
      		<li>
      			<%= link_to("Sign Up", {controller: :dancers, action: :new}, class: "btn btn-primary navbar-btn") %>
      		</li>
      	</ul>
			</div>
		</div>
	</nav>
				<%= yield %>

</body>
</html>
